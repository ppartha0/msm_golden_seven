<h1>All Directors</h1>

<!DOCTYPE html>
<html>
  <head>
    <title>Must See Movies Golden Seven</title>
    <!-- Latest compiled and minified CSS -->
    
    <link rel="stylesheet" media="all" href="/assets/application-1c79c29286204cfec74780022d0186e27fc9f09c8b5ae96d566e54dca4b9ce48.css" />
    <script src="/assets/application-a1b05e0595224c422d49fd5ef6dd6cb8f9655f710b432bb9b8a6af9638eca09a.js"></script>
  </head>

<body>
<div class="container">
  <div class="col-md-6">
    <h1>All Movies</h1>


    <a href="/directors/new">
      Add a new director
    </a>


    <% @p.each do |dbrow| %>
        <div class="thumbnail">
            <img src="<%= dbrow.image_url %>" >
            <div class="caption">
                 <p><%= dbrow.name %></p>
                 <p>Summary: <%= dbrow.bio %></p>
                 <p>Date Of Birth: <%= dbrow.dob %></p>
            </div>  
        
        </div>
    <% end %>
    </div>
</div>
</body>
</html>
    

